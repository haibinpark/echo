<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="0px" android:color="@color/keyboard_divide_color" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/keyboard_selector_color" />
            <stroke android:width="1px" android:color="@color/keyboard_divide_color" />
        </shape>
    </item>
</selector>